CC = gcc
SRC = main.c school.c
OBJ = $(patsubst %.c,%.o,$(SRC))
NAME = school
RM = rm -f
all : $(OBJ)
	$(CC) $(OBJ) -o $(NAME)
clean :
	$(RM) *~ $(NAME)
oclean :
	$(RM) $(OBJ)
fclean :
	$(RM) *~ $(NAME) $(OBJ)
re : oclean $(OBJ)
	$(CC) $(OBJ) -o $(NAME)
